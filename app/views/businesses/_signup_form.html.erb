<fieldset>
  <div class="row-fluid">
    <div class="span6">
      <label>Full Name of User</label>
      <%= f.text_field :fullname -%>
    </div>
    <div class="span6">
      <label>Business Name</label>
      <%= f.text_field :business_name -%>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span6">
      <label>Email</label>
      <%= f.text_field :email -%>
    </div>
    <div class="span6">
      <label>Business URL</label>
      <%= f.text_field :business_url -%>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span6">
      <label>Password</label>
      <%= f.password_field :password -%>
    </div>
    <div class="span6">
      <label>Phone number</label>
      <%= f.text_field :phone_number -%>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span6">
      <label>Password Confirmation</label>
      <%= f.password_field :password_confirmation -%>
    </div>
    <div class="span6">
      <button class="btn btn-yellow w80 pull-right" type="submit" style="margin-top: 25px;">
        Signup
      </button>
    </div>
  </div>
  <hr class="m-bot0">
</fieldset>
